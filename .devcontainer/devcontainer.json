{
	"name": "gnome-44-python-devel",
	"image": "quay.io/gnome_infrastructure/gnome-runtime-images:gnome-44",
	"shutdownAction": "stopContainer",
	"postCreateCommand": "bash .devcontainer/on_create.sh",
	"remoteUser": "root",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.isort"
			]
		}
	}
}
