<interface>
  <requires lib="gtk" version="4.6"/>
  <requires lib="libadwaita" version="1.1"/>
  <template class="SingleWindow" parent="AdwApplicationWindow">
    <property name="default-height">40</property>
    <property name="default-width">180</property>
    <child>
      <object class="GtkOverlay" id="_overlay">
        <property name="child">
          <object class="GtkBox" id="_monitors_box">
            <property name="margin-bottom">8</property>
            <property name="margin-end">8</property>
            <property name="margin-start">8</property>
            <property name="margin-top">8</property>
            <property name="orientation">vertical</property>
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
